{
  "name": "from_scratch",
  "version": "0.1.0",
  "private": true,
  "main": "dist/server",
  "scripts": {
    "start": "node .",
    "env:dev": "cross-env NODE_ENV=development",
    "env:prod": "cross-env NODE_ENV=production",
    "env:example": "cross-env SOURCE=src-example",
    "lint": "eslint src private webpack.config.js",
    "storybook": "start-storybook -p 9001 -c private/storybook",
    "predev": "npm run prebuild",
    "dev": "npm run env:dev -- webpack-dev-server",
    "dev:example": "npm run env:example -- npm run dev",
    "build:clean": "rimraf \"dist/!(.git*|Procfile)**\"",
    "build:copy": "copyfiles public/* public/**/* dist",
    "prebuild": "npm run build:clean && npm run build:copy",
    "build": "npm run env:prod -- webpack"
  },
  "devDependencies": {
    "@webpack-blocks/dev-server2": "^0.4.0",
    "@webpack-blocks/webpack2": "^0.4.0",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-styled-components": "^1.1.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.0",
    "babel-preset-env": "^1.3.3",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "copyfiles": "^1.0.0",
    "eslint": "^4.3.0",
    "eslint-plugin-react": "^7.1.0",
    "file-loader": "^0.11.0",
    "html-webpack-plugin": "^2.26.0",
    "stylelint": "^8.0.0",
    "url-loader": "^0.5.7",
    "webpack": "^3.4.0",
    "webpack-assets-by-type-plugin": "^0.1.0",
    "webpack-blocks-happypack": "^0.1.3",
    "webpack-blocks-server-source-map": "^0.2.0",
    "webpack-blocks-split-vendor": "^0.2.1",
    "webpack-child-config-plugin": "^0.1.1",
    "webpack-dev-server": "^2.3.0",
    "webpack-node-externals": "^1.6.0",
    "webpack-spawn-plugin": "^0.1.2"
  },
  "dependencies": {
    "babel-polyfill": "^6.20.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "history": "^4.6.3",
    "install": "^0.10.1",
    "lodash": "^4.17.4",
    "material-ui": "^0.18.6",
    "normalizr": "^3.0.1",
    "npm": "^5.3.0",
    "prop-types": "^15.5.8",
    "query-string": "^4.3.2",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-hot-loader": "3.0.0-beta.6",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.2",
    "react-router-redux": "^5.0.0-alpha.6",
    "react-router-server": "^4.2.1",
    "react-tap-event-plugin": "^2.0.1",
    "react-youtube": "^7.4.0",
    "redux": "^3.6.0",
    "redux-form": "^6.4.3",
    "serialize-javascript": "^1.4.0",
    "styled-components": "^2.0.0",
    "styled-theme": "^0.3.0",
    "styled-tools": "^0.1.2",
    "validator": "^7.0.0"
  }
}
